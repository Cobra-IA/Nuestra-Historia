body {
    font-family: Arial, sans-serif;
    background-color: #121212;
    color: #e0e0e0;
    text-align: center;
    padding: 20px;
}

h1 {
    color: #ff9800;
}

p {
    font-size: 18px;
}

a {
    color: #ff9800;
    text-decoration: none;
}

a:hover {
    text-decoration: underline;
}
footer {
    text-align: center;
    padding: 10px;
    background-color: #f1f1f1;
    position: fixed;
    bottom: 0;
    width: 100%;
}
